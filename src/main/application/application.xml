          <parameter id="bbox" title="End Year" abstract="End date of the TOI (e.g. 2014-01-11T09:36:00). Used as queryable for the Landsat 8 catalogue" scope="runtime" type="opensearch" target="time:end">2014</parameter>          
      </defaultParameters>
      <defaultJobconf>
          <property id="mapred.task.timeout">600000000</property>
